<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.soecode.lyf.dao.deal.AppointmentDao">
  <resultMap id="BaseResultMap" type="AppointmentDO">
    <result column="book_id" jdbcType="BIGINT" property="bookId" />
    <result column="student_id" jdbcType="BIGINT" property="studentId" />
    <result column="link_list" jdbcType="VARCHAR"  property="linkInfoDOList" typeHandler="LinkInfoHandler" />
  </resultMap>
  <sql id="Base_Column_List">
    book_id, student_id, link_list
  </sql>
  <insert id="insert" parameterType="AppointmentDO">
    insert into appointment (book_id, student_id, link_list)
    values (#{bookId,jdbcType=BIGINT}, #{studentId,jdbcType=BIGINT}, #{linkInfoDOList,typeHandler=LinkInfoHandler})
  </insert>
  <select id="selectAll" resultMap="BaseResultMap">
    select <include refid="Base_Column_List" /> from appointment
  </select>
</mapper>